	if(not(explicitRelativePathName.oclIsUndefined()) and (explicitRelativePathName<>''))then{
//		relativePathName:=explicitRelativePathName;
		pathURI:=explicitRelativePathName;
	}endif;
//	descriptionText:=pimmpd.getFileDescriptionText();

<plugin>
   <extension
         point="org.eclipse.m2m.qvt.oml.javaBlackboxUnits">
		  <unit name="niem" namespace="org.modeldriven.magicdraw.niem.qvt">
   		
    		<library name="mpd" class="org.modeldriven.magicdraw.niem.qvt.Blackbox">
		      <metamodel nsURI="http://www.eclipse.org/emf/2002/Ecore"/>
		      <metamodel nsURI="http://www.eclipse.org/uml2/3.0.0/UML"/>
		      <metamodel nsURI="http://www.eclipse.org/xsd/2002/XSD"/>
		      <metamodel nsURI="http://www.eclipse.org/MoDisco/Xml/0.1.incubation/XML"/>
    		</library>
		</unit>
   </extension>
  <extension
        id="NIEMQVT"
        name="NIEM QVT"
        point="org.eclipse.gmf.runtime.emf.core.Pathmaps">
     <pathmap
           name="NIEM_TRANSFORMS"
           path="transform"
           plugin="org.modeldriven.magicdraw.niem.qvt"/>
  </extension>
    <extension
     point="org.eclipse.emf.ecore.uri_mapping">
    <mapping
       source="pathmap://NIEM_TRANSFORMS/"
       target="transform/">
    </mapping>
  </extension>
  
</plugin>

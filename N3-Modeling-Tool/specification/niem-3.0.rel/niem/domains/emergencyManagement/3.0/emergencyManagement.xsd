        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Other">
        <xs:annotation>
          <xs:documentation>Other events</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Rescue">
        <xs:annotation>
          <xs:documentation>Rescue and Recovery</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Safety">

  <xs:simpleType name="AlertEventDetailsCertaintyCodeSimpleType">
    <xs:annotation>
      <xs:documentation>A data type for the certainty code designated to an alert event.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:enumeration value="Likely">
        <xs:annotation>
          <xs:documentation>Likely greater than ~50%</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Observed">
        <xs:annotation>
          <xs:documentation>Determined to have occurred or to be ongoing</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Possible">
        <xs:annotation>
          <xs:documentation>Possible but not likely (less than or equal to 50%)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Unknown">
        <xs:annotation>
          <xs:documentation>Certainty unknown</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Unlikely">
        <xs:annotation>
          <xs:documentation>Not expected to occur (~0)</xs:documentation>
